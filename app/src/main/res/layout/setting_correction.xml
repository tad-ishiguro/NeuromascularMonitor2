        <EditTextPreference
            android:key="blocks_one_stim_key"
            android:summary="@string/blocks_one_stim"
            android:title="@string/blocks_one_stim"
            android:inputType="numberDecimal"
            android:digits="0123456789."
            android:maxLength="6"   />

        <EditTextPreference
            android:key="recovery_half_life_key"
            android:summary="@string/recovery_half_life"
            android:title="@string/recovery_half_life"
            android:inputType="numberDecimal"
            android:digits="0123456789."
            android:maxLength="6"   />

        <EditTextPreference
            android:key="PTP_key"
            android:summary="@string/PTP"
            android:title="@string/PTP"
            android:inputType="numberDecimal"
            android:digits="0123456789."
            android:maxLength="6"   />

        <EditTextPreference
            android:key="PTP_Half_life_key"
            android:summary="@string/PTP_Half_life"
            android:title="@string/PTP_Half_life"
            android:inputType="numberDecimal"
            android:digits="0123456789."
            android:maxLength="6"   />

        <CheckBoxPreference
            android:defaultValue="0"
            android:key="correction_log_key"
            android:title="@string/correction_log_check" />


    </PreferenceCategory>
</PreferenceScreen>